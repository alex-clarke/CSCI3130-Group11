<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.csci3130_group11.csci3130_group11.DisplayCurrentData">

    <TextView
        android:id="@+id/TEMPERATURE_LABEL"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="32dp"
        android:text="Temperature"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LIGHT_LABEL" />

    <TextView
        android:id="@+id/HUMIDITY_LABEL"
        android:layout_width="67dp"
        android:layout_height="17dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="75dp"
        android:text="Humidity"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/change_range_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set Ranges"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/CURRENT_LABEL"
        android:layout_width="61dp"
        android:layout_height="17dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="50dp"
        android:text="Current"
        android:textAlignment="textStart"
        app:layout_constraintLeft_toRightOf="@+id/MIN_LABEL"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/MIN_LABEL"
        android:layout_width="57dp"
        android:layout_height="17dp"
        android:layout_marginLeft="116dp"
        android:layout_marginTop="50dp"
        android:text="Min"
        android:textAlignment="textStart"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/MAX_LABEL"
        android:layout_width="30dp"
        android:layout_height="17dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="50dp"
        android:text="Max"
        android:textAlignment="textStart"
        app:layout_constraintLeft_toRightOf="@+id/CURRENT_LABEL"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/LIGHT_LABEL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="32dp"
        android:text="Light"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HUMIDITY_LABEL" />

    <TextView
        android:id="@+id/MIN_T_RANGE"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="33dp"
        android:layout_marginLeft="116dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MIN_L_RANGE" />

    <TextView
        android:id="@+id/CURRENT_T"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="32dp"
        app:layout_constraintLeft_toRightOf="@+id/MIN_T_RANGE"
        android:layout_marginLeft="31dp"
        app:layout_constraintTop_toBottomOf="@+id/CURRENT_L" />

    <TextView
        android:id="@+id/MAX_T_RANGE"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="32dp"
        app:layout_constraintLeft_toRightOf="@+id/CURRENT_T"
        android:layout_marginLeft="32dp"
        app:layout_constraintTop_toBottomOf="@+id/MAX_L_RANGE" />

    <TextView
        android:id="@+id/MIN_H_RANGE"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MIN_LABEL"
        android:layout_marginLeft="116dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/CURRENT_H"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/CURRENT_LABEL"
        app:layout_constraintLeft_toRightOf="@+id/MIN_H_RANGE"
        android:layout_marginLeft="32dp" />

    <TextView
        android:id="@+id/MAX_H_RANGE"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MAX_LABEL"
        app:layout_constraintLeft_toRightOf="@+id/CURRENT_H"
        android:layout_marginLeft="32dp" />

    <TextView
        android:id="@+id/MIN_L_RANGE"
        android:layout_width="61dp"
        android:layout_height="16dp"
        android:text="TextView"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/MIN_H_RANGE"
        android:layout_marginLeft="116dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/CURRENT_L"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:text="TextView"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/CURRENT_H"
        app:layout_constraintLeft_toRightOf="@+id/MIN_L_RANGE"
        android:layout_marginLeft="28dp" />

    <TextView
        android:id="@+id/MAX_L_RANGE"
        android:layout_width="58dp"
        android:layout_height="17dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/CURRENT_L"
        app:layout_constraintTop_toBottomOf="@+id/MAX_H_RANGE" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="15dp"
        android:layout_height="16dp"
        android:layout_marginRight="11dp"
        android:text="Â°C"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintLeft_toRightOf="@+id/MAX_T_RANGE"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintLeft_toRightOf="@+id/MAX_L_RANGE"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="75dp"
        app:layout_constraintLeft_toRightOf="@+id/MAX_H_RANGE"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="16dp"
        android:orientation="horizontal" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="vertical" />

    <TextView
        android:id="@+id/WarningTextView"
        android:layout_width="278dp"
        android:layout_height="132dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/change_range_button" />

</android.support.constraint.ConstraintLayout>
